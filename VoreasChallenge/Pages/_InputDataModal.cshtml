@{
}

@model Models.InputData
<!-- Modal -->
<div class="modal fade" id="add-contact" tabindex="-1" role="dialog" aria-labelledby="addContactLabel" aria-hidden="true">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title" id="addContactLabel">データ入力</h5>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">×</span>
				</button>
			</div>
			<div class="modal-body">
				<form asp-page-handler="InputDataModal">
					<input name="IsValid" type="hidden" value="@ViewData.ModelState.IsValid.ToString()" />

					<div class="form-group">
						<label asp-for="Id"></label>
						<input asp-for="Id" class="form-control" />
						<span asp-validation-for="Id" class="text-danger"></span>
					</div>

					<!--個人情報-->
					<div class="form-group">
						<label asp-for="Name"></label>
						<input asp-for="Name" class="form-control" />
						<span asp-validation-for="Name" class="text-danger"></span>
					</div>
					<div class="form-group">
						<label asp-for="SportsType"></label>
						<input asp-for="SportsType" class="form-control" />
						<span asp-validation-for="SportsType" class="text-danger"></span>
					</div>
					<div class="form-group">
						<label asp-for="MeasureDay"></label>
						<input asp-for="MeasureDay" class="form-control" />
						<span asp-validation-for="MeasureDay" class="text-danger"></span>
					</div>
					<div class="form-group">
						<label asp-for="Grade"></label>
						<input asp-for="Grade" class="form-control" />
						<span asp-validation-for="Grade" class="text-danger"></span>
					</div>
					<div class="form-group">
						<label asp-for="Sex"></label>
						<input asp-for="Sex" class="form-control" />
						<span asp-validation-for="Sex" class="text-danger"></span>
					</div>
					<div class="form-group">
						<label asp-for="BirthDay"></label>
						<input asp-for="BirthDay" class="form-control" />
						<span asp-validation-for="BirthDay" class="text-danger"></span>
					</div>

					<!--体格情報-->
					<div class="form-group">
						<label asp-for="Height"></label>
						<input asp-for="Height" class="form-control" />
						<span asp-validation-for="Height" class="text-danger"></span>
					</div>
					<div class="form-group">
						<label asp-for="ShittingHeight"></label>
						<input asp-for="ShittingHeight" class="form-control" />
						<span asp-validation-for="ShittingHeight" class="text-danger"></span>
					</div>
					<div class="form-group">
						<label asp-for="LowerLimbLength"></label>
						<input asp-for="LowerLimbLength" class="form-control" />
						<span asp-validation-for="LowerLimbLength" class="text-danger"></span>
					</div>
					<div class="form-group">
						<label asp-for="Weight"></label>
						<input asp-for="Weight" class="form-control" />
						<span asp-validation-for="Weight" class="text-danger"></span>
					</div>
					<div class="form-group">
						<label asp-for="BodyFat"></label>
						<input asp-for="BodyFat" class="form-control" />
						<span asp-validation-for="BodyFat" class="text-danger"></span>
					</div>

					<!--計測情報-->
					<div class="form-group">
						<label asp-for="Run20m"></label>
						<input asp-for="Run20m" class="form-control" />
						<span asp-validation-for="Run20m" class="text-danger"></span>
					</div>
					<div class="form-group">
						<label asp-for="ProAgility"></label>
						<input asp-for="ProAgility" class="form-control" />
						<span asp-validation-for="ProAgility" class="text-danger"></span>
					</div>
					<div class="form-group">
						<label asp-for="StandJump"></label>
						<input asp-for="StandJump" class="form-control" />
						<span asp-validation-for="StandJump" class="text-danger"></span>
					</div>
					<div class="form-group">
						<label asp-for="RepetJump"></label>
						<input asp-for="RepetJump" class="form-control" />
						<span asp-validation-for="RepetJump" class="text-danger"></span>
					</div>
					<div class="form-group">
						<label asp-for="VerticalJump"></label>
						<input asp-for="VerticalJump" class="form-control" />
						<span asp-validation-for="VerticalJump" class="text-danger"></span>
					</div>
					<div class="form-group">
						<label asp-for="GCTime"></label>
						<input asp-for="GCTime" class="form-control" />
						<span asp-validation-for="GCTime" class="text-danger"></span>
					</div>
					<div class="form-group">
						<label asp-for="JumpHeight"></label>
						<input asp-for="JumpHeight" class="form-control" />
						<span asp-validation-for="JumpHeight" class="text-danger"></span>
					</div>
				</form>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-secondary" data-dismiss="modal">閉じる</button>
				<button type="button" class="btn btn-primary" data-save="modal">保存</button>
			</div>
		</div>
	</div>
</div>
